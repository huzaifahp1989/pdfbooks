{
  "buildCommand": "true",
  "outputDirectory": ".",
  "devCommand": "python -m http.server 8000",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(?!.*\\.)",
      "destination": "/index.html"
    }
  ]
}
